<forge-select
  label="<%= control.label %>"
  id="<%= control.id %>"
  <%= control.multiple ? 'multiple': null %>
  <%= control.disabled ? 'disabled' : null %>
  value="<%= control.defaultValue ?? undefined %>">
  <% control.options.forEach(({ label, value }) => { %>
    <forge-option value="<%= value %>"><%= label %></forge-option>
  <% }); %>
</forge-select>
