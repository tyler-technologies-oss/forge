{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["es2022", "dom", "dom.iterable"],
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "allowSyntheticDefaultImports": true,
    "useDefineForClassFields": false,
    "skipLibCheck": true
  }
}
